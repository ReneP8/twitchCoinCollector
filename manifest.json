{
    "manifest_version": 2,
    "name": "Twitch Point Collector",
    "description": "A Chrome Extension to collect the channel points",
    "version": "0.1",
    "permissions": [
        "<all_urls>"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}